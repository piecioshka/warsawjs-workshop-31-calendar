{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "data"
    ],
    "properties": {
        "data": {
            "$id": "#/properties/data",
            "type": "array",
            "title": "The Data Schema",
            "items": {
                "$id": "#/properties/data/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                    "id",
                    "description",
                    "time",
                    "title",
                    "notification"
                ],
                "properties": {
                    "id": {
                        "$id": "#/properties/data/items/properties/id",
                        "type": "string",
                        "title": "The Id Schema",
                        "default": "",
                        "examples": [
                            "5ca867948d0d701a9dfc6cff"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "description": {
                        "$id": "#/properties/data/items/properties/description",
                        "type": "string",
                        "title": "The Description Schema",
                        "default": "",
                        "examples": [
                            "asdasd"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "time": {
                        "$id": "#/properties/data/items/properties/time",
                        "type": "string",
                        "title": "The Time Schema",
                        "default": "",
                        "examples": [
                            "2019-04-11T12:00"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "title": {
                        "$id": "#/properties/data/items/properties/title",
                        "type": "string",
                        "title": "The Title Schema",
                        "default": "",
                        "examples": [
                            "asdasd"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "notification": {
                        "$id": "#/properties/data/items/properties/notification",
                        "type": "boolean",
                        "title": "The Notification Schema",
                        "default": false,
                        "examples": [
                            false
                        ]
                    }
                }
            }
        }
    }
}